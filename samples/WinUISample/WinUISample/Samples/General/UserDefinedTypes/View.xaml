<UserControl
    x:Class="WinUISample.General.UserDefinedTypes.View"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:lvc="using:LiveChartsCore.SkiaSharpView.WinUI"
    xmlns:vms="using:ViewModelsSamples.General.UserDefinedTypes"
    xmlns:local="using:WinUISample.General.UserDefinedTypes"
    mc:Ignorable="d">

    <UserControl.DataContext>
        <vms:ViewModel/>
    </UserControl.DataContext>

    <lvc:CartesianChart>
        <lvc:CartesianChart.Series>
            <local:CustomCityLineSeries
                SeriesName="Population"
                Values="{Binding Values}"
                YToolTipLabelFormatter="{Binding TooltipFormatter}"/>
        </lvc:CartesianChart.Series>
    </lvc:CartesianChart>

</UserControl>

<UserControl
    x:Class="WPFSample.General.UserDefinedTypes.View"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:lvc="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
    xmlns:vms="clr-namespace:ViewModelsSamples.General.UserDefinedTypes;assembly=ViewModelsSamples"
    xmlns:local="clr-namespace:WPFSample.General.UserDefinedTypes">

    <UserControl.DataContext>
        <vms:ViewModel/>
    </UserControl.DataContext>

    <lvc:CartesianChart>
        <lvc:CartesianChart.Series>
            <lvc:SeriesCollection>
                <local:CustomCityLineSeries
                    SeriesName="Population"
                    Values="{Binding Values}"
                    YToolTipLabelFormatter="{Binding TooltipFormatter}"/>
            </lvc:SeriesCollection>
        </lvc:CartesianChart.Series>
    </lvc:CartesianChart>

</UserControl>
